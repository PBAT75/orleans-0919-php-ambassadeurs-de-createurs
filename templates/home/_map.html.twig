{% block map %}

    <div class="js-ambassadors" data-ambassadors="{{ ambassadors|json_encode }}"></div>
    <div class="js-events" data-events="{{ events|json_encode }}"></div>
    <div class="d-flex w-100 justify-content-center map-containeur mt-3">
        <div class="d-flex flex-column mt-5 mapTabs align-self-center">
            <h2 class="align-self-center m-lg-4 homepageTitle"> Une communauté près de chez vous</h2>
            <div class="row justify-content-around mb-3 mt-5">
                <a class="btn btn-search active col-lg-4 col-10 mb-4" id="events" href="#events">
                    <i class="fas fa-map-marker-alt"></i>
                      Prochaines ventes privées
                </a>
                <a class="btn btn-search col-lg-4 col-10 mb-4" id="ambassadors" href="#ambassadors">
                    <i class="fas fa-map-marker-alt"></i>
                    Ambassadeurs</a>
            </div>
            <div class="mt-1" id="map"></div>
        </div>
    </div>
{% endblock %}
